{
    "manifest_version": 3,
    "name": "Todo List",
    "version": "1.0.0",
    "description": "A Todo List I Built To Increase My Productivity",
    "icons": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png",
      "128": "128.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "alarms",
      "notifications"
    ]
  }
  